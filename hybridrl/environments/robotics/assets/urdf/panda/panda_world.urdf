<?xml version="1.0" encoding="utf-8"?>
<robot name="panda_world">
    <material name="dark_gray"><color rgba="0.36 0.36 0.38 1.0"/></material>
    <material name="white"><color rgba="1.0 1.0 1.0 1.0"/></material>

    <link name="floor">
        <inertial>
            <mass value="2000.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
        <visual>
            <origin xyz="0.25 0.0 -0.005" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="1.1 0.7 0.01"/>
            </geometry>
            <material name="dark_gray"/>
        </visual>
        <collision>
            <origin xyz="0.25 0.0 -0.005" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="1.1 0.7 0.01"/>
            </geometry>
        </collision>
    </link>  
    <joint name="floor_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="floor"/>
        <child link="table"/>
    </joint>
    <link name="table">
        <contact>
            <lateral_friction value="1"/>
        </contact>
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="2000.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
        <visual>
            <origin xyz="0.5 0.0 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.5 0.5 0.1"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0.5 0.0 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.5 0.5 0.1"/>
            </geometry>
        </collision>
    </link>
</robot>
